// main.vsif
session test_nesting {
    session_description: <text>Test nesting</text>;
    drm: "lsf";
    seed: random;
};

group level_one {
    count: 100;

    test reg_irq    { top_files: "test.e";  count: 1; };
    group level_two {
        test foo { seed: 1; };

        group level_three {
            attrib: "nothing";
            test nil { count: 0; };
        };

        test bar { seed: 2; };
    };

    extend level_two {
        count: 10;
        extend bar { count: 1; sanity_seed: random;};
        extend foo { count: 2; sanity_count: 99;};
    };
};
